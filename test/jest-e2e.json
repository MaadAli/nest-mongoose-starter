{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@/(.*)": "<rootDir>/src/$1"
  },
  "testPathIgnorePatterns": [".d.ts", ".js"],
  "setupFilesAfterEnv": ["jest-extended/all"]

}
